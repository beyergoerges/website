extends default.pug

block vars
  - var bodyClass = "jd-portfolio";

block svg 
  include includes/svg/clients.pug
  include includes/svg/press.pug

block content
  header.jd-block.jd-block--intro
    h1.jd-heading.jd-heading--page Werte in einer digitalen Welt
    h2.jd-heading.jd-heading--high-level Wir sind eine multidisziplinäre Digital-Agentur. Gegründet 2018.
  .jd-block.jd-block--solid.jd-block--color-1
    section.jd-section#projects
      h3.jd-heading.jd-heading--section.jd-section__title.jd-section__title--followed-by-text
        span.jd-section__title-index
          span.jd-section__title-number 01
          span.jd-section__title-line
        span.jd-section__title-text Strategie
      .jd-craft__text
        h2.jd-heading.jd-heading--high-level.jd-craft__heading.--color-1 Das große Ganze
        p.jd-paragraph.jd-craft__description
          | In unserer schnelllebigen Zeit ist es verführerisch, stets den neuesten Trends zu folgen, um kurzfristig erfolgreich zu sein. Wir helfen die Dinge einzuordnen und entwickeln gemeinsam digitale Strategien mit Weitblick.
      ul.jd-data-list.jd-craft__list
        li.jd-data-list__item Beratung
        li.jd-data-list__item Digitale Transformation
        li.jd-data-list__item Konzeption
        li.jd-data-list__item Content-Strategie
        li.jd-data-list__item Branding & Identität
        li.jd-data-list__item Recherche & Analyse
    section.jd-section.jd-craft#design
      h3.jd-heading.jd-heading--section.jd-section__title.jd-section__title--followed-by-heading
        span.jd-section__title-index
          span.jd-section__title-number 02
          span.jd-section__title-line
        span.jd-section__title-text Presse
      .jd-press__items
        a.jd-press__item(href="https://www.sueddeutsche.de/sport/bayern-gegen-dortmund-im-netz-die-fussgaengerzone-der-welt-1.2200876" target="_blank")
          svg.jd-press__logo.jd-press__logo--sz
            use(href="#logo-sz" xlink:href="#logo-sz")
        a.jd-press__item(href="https://www.wired.com/2011/01/cinema-out-of-your-backpack/" target="_blank")
          svg.jd-press__logo.jd-press__logo--wired
            use(href="#logo-wired" xlink:href="#logo-wired")
        //- a.jd-press__item(href="" target="_blank")
        //-   svg.jd-press__logo.jd-press__logo--sponsors
        //-     use(href="#logo-sponsors" xlink:href="#logo-sponsors")
        a.jd-press__item(href="http://www.faz.net/aktuell/sport/mehr-sport/woran-die-bundesliga-beim-thema-e-sport-noch-scheitert-15749191.html" target="_blank")
          svg.jd-press__logo.jd-press__logo--faz
            use(href="#logo-faz" xlink:href="#logo-faz")
        a.jd-press__item(href="https://www.11freunde.de/interview/ein-film-zur-meisterfeier-des-bvb" target="_blank")
          svg.jd-press__logo.jd-press__logo--11freunde
            use(href="#logo-11freunde" xlink:href="#logo-11freunde")
        a.jd-press__item(href="https://www.ruhrnachrichten.de/Nachrichten/Dortmund/Intensives-Jubeln-intensives-Leiden-der-BVB-im-Social-Web-17689.html" target="_blank")
          svg.jd-press__logo.jd-press__logo--rn
            use(href="#logo-rn" xlink:href="#logo-rn")
        a.jd-press__item(href="https://www.handelsblatt.com/sport/fussball/digitalisierung-im-fussball-mit-einem-bein-im-show-geschaeft/20019142-all.html?ticket=ST-6016452-WHYa02HeLakYO0DBHcSj-ap3" target="_blank")
          svg.jd-press__logo.jd-press__logo--handelsblatt
            use(href="#logo-handelsblatt" xlink:href="#logo-handelsblatt")
        a.jd-press__item(href="https://techcrunch.com/2011/01/02/fancy-yourself-a-dslr-filmmaker-heres-your-change-to-prove-it/" target="_blank")
          svg.jd-press__logo.jd-press__logo--techcrunch
            use(href="#logo-techcrunch" xlink:href="#logo-techcrunch")
    section.jd-section.jd-press#speaker
      h3.jd-heading.jd-heading--section.jd-section__title
        span.jd-section__title-index
          span.jd-section__title-number 03
          span.jd-section__title-line
        span.jd-section__title-text Vorträge
      .jd-press__items
        a.jd-press__item(href="https://re-publica.com/en/member/9132" target="_blank")
          svg.jd-press__logo.jd-press__logo--republica
            use(href="#logo-republica" xlink:href="#logo-republica")
        a.jd-press__item(href="https://www.campus-symposium.com" target="_blank")
          img.jd-press__logo.jd-press__logo--campus(src="images/press/campus.png" alt="Campus Symposium")
        //- a.jd-press__item(href="" target="_blank")
        //-   img.jd-press__logo.jd-press__logo--faz-forum(src="images/press/faz-forum.png" alt="Frankfurter Allgemeine Forum")
        a.jd-press__item(href="https://www.omk2018.de/speaker/david-goerges/" target="_blank")
          img.jd-press__logo.jd-press__logo--omk(src="images/press/omk.png" alt="OMK")
        a.jd-press__item(href="https://www.netandwork.de/speaker" target="_blank")
          img.jd-press__logo.jd-press__logo--netandwork(src="images/press/netandwork.png" alt="Net&Work")
  .jd-block.jd-block--outro
    a.jd-heading.jd-heading--page.jd-heading--inverted(href="/agency")
      span.jd-heading__text Agentur
      span.jd-arrow
